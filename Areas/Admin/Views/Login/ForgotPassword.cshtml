
@{
    ViewData["Title"] = "ForgotPassword";
    Layout = null;
}
@*<a href="~/lib/bootstrap/dist/css/bootstrap.min.css.map">~/lib/bootstrap/dist/css/bootstrap.min.css.map</a>*@
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
@*<a href="~/lib/bootstrap/dist/js/bootstrap.min.js.map">~/lib/bootstrap/dist/js/bootstrap.min.js.map</a>*@
<script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
<h4>ForgotPassword</h4>
<hr />
<div class="container">
    <form asp-action="ResetPassword" method="post">
    <div class="form-group">
        <label class="form-label" >Email</label>
        <input type="email" required name="email"  class="form-control" />
    </div> <br />
    <input type="submit" class="btn btn-warning" value="Reset password" />
</form>
</div>



